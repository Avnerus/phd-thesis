@article{derricoProsocialVirtualReality2020,
  title = {Prosocial {{Virtual Reality}}, {{Empathy}}, and {{EEG Measures}}: {{A Pilot Study Aimed}} at {{Monitoring Emotional Processes}} in {{Intergroup Helping Behaviors}}},
  shorttitle = {Prosocial {{Virtual Reality}}, {{Empathy}}, and {{EEG Measures}}},
  author = {D'Errico, Francesca and Leone, Giovanna and Schmid, Maurizio and D'Anna, Carmen},
