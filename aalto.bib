  shorttitle = {An {{Integrated World Modeling Theory}} ({{IWMT}}) of {{Consciousness}}},
  author = {Safron, Adam},
  year = {2020},
  journal = {Frontiers in Artificial Intelligence},
  volume = {3},
  pages = {30},
