  volume = {21},
  number = {1},
  pages = {131--146},
  issn = {1571-9979},
  doi = {10.1111/j.1571-9979.2005.00050.x},
  urldate = {2020-04-23},
  abstract = {This article examines a new approach for evaluating the process of interactive conflict-resolution interventions by focusing on the quality of communication between the groups in dispute. This approach assesses the extent to which contact interventions, aimed at improving relations between sides in dispute, actually promote among their participants relationships, behaviors, and interactions that fulfill standards of social justice, equality, and fairness. Creating equality in communication models and reinforces values of mutual respect and tolerance. Thus, establishing symmetrical communication can be especially important for many interventions conducted to resolve conflicts marked by unequal distribution of power. One such asymmetrical conflict is the one between the Jewish majority and the Arab minority in Israel. This article describes a study that applies our process-evaluation approach to assess equality and social justice in planned contact interventions between Israeli Jews and Arabs by measuring the degree to which members of the two groups contributed equally to the communication between them. Finally, the article presents guidelines, derived from this and other similar studies, for establishing communication equality in other planned contact interventions.},
  langid = {english},
  file = {C\:\\Users\\avner\\Zotero\\storage\\UQ5E8JXL\\Maoz - 2005 - Evaluating the Communication between Groups in Dis.pdf;C\:\\Users\\avner\\Zotero\\storage\\VD6CLFH5\\j.1571-9979.2005.00050.html}
}

@article{maozMultipleConflictsCompeting2000,
  title = {Multiple {{Conflicts}} and {{Competing Agendas}}: {{A Framework}} for {{Conceptualizing Structured Encounters Between Groups}} in {{Conflict-The Case}} of a {{Coexistence Project}} of {{Jews}} and {{Palestinians}} in {{Israel}}},
  shorttitle = {Multiple {{Conflicts}} and {{Competing Agendas}}},
  author = {Maoz, Ifat},
  year = {2000},
  month = jun,
  journal = {Peace and Conflict: Journal of Peace Psychology},
  volume = {6},
  number = {2},
  pages = {135--156},
  publisher = {Routledge},
  issn = {1078-1919},
  doi = {10.1207/S15327949PAC0602_3},
  urldate = {2024-11-21},
  abstract = {This study presents a framework for conceptualizing processes of interaction and communication within structured encounters between groups in conflict. It is based on the assumption that such encounters essentially reflect or are influenced by the external reality of conflict. As such, they are likely to be characterized by multiple interacting conflicts (Bar-On, 1997); by disputes over goals, agendas, and the expression of group identities; and by patterns of influence and control. This paradigm was used to analyze a case of structured coexistence encounters between Jews and Palestinians in Israel. The analysis of the processes and the dynamics of these encounters reveals a configuration of groups, subgroups, and coalitions that evolved around the central dispute over the encounters' agenda. This dispute reflected and interacted with the ethnonational conflict between Jews and Palestinians. It appears that the notion of cooperation so successfully employed in Sherif's (1966) study acquired a different meaning when transposed to a setting of unofficial third-party intervention in a genuine ethnonational conflict. Like similar agendas promoting coexistence and cooperation, the official agenda forwarded by the Jewish management of joint pedagogical activity was perceived by the Palestinian participants as an attempt to prevent the minority from expressing its national identity and demands for recognition. The power struggles that revolved around this dispute over the agenda are described, and the implications of the paradigm for the theory and practice of interventions aimed at fostering coexistence and peace building are discussed.}
}

