@article{abuzerrImpactCoronavirusDisease2021,
  title = {Impact of the Coronavirus Disease 2019 Pandemic on the {{Palestinian}} Family: {{A}} Cross-Sectional Study},
  shorttitle = {Impact of the Coronavirus Disease 2019 Pandemic on the {{Palestinian}} Family},
  author = {Abuzerr, Samer and Zinszer, Kate and Shaheen, Amira and {el Bilbeisi}, Abdel Hamid and Al Haj Daoud, Ayman and Aldirawi, Ali and Salem, Alshaarawi},
