@article{dixonNegativeContactCollective2021,
  title = {Negative Contact, Collective Action, and Social Change: {{Critical}} Reflections, Technological Advances, and New Directions},
  shorttitle = {Negative Contact, Collective Action, and Social Change},
  author = {Dixon, John and McKeown, Shelley},
  year = {2021},
  journal = {Journal of Social Issues},
  volume = {77},
