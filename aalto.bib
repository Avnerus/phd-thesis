  title = {A {{Fully Fabric-Based Bidirectional Soft Robotic Glove}} for {{Assistance}} and {{Rehabilitation}} of {{Hand Impaired Patients}}},
  author = {Yap, H. K. and Khin, P. M. and Koh, T. H. and Sun, Y. and Liang, X. and Lim, J. H. and Yeow, C.},
