@article{ereteMethodMadnessApplying2023,
  title = {A {{Method}} to the {{Madness}}: {{Applying}} an {{Intersectional Analysis}} of {{Structural Oppression}} and {{Power}} in {{HCI}} and {{Design}}},
  shorttitle = {A {{Method}} to the {{Madness}}},
  author = {Erete, Sheena and Rankin, Yolanda and Thomas, Jakita},
