@article{tassinariInvestigatingInfluenceIntergroup2022,
  title = {Investigating the {{Influence}} of {{Intergroup Contact}} in {{Virtual Reality}} on {{Empathy}}: {{An Exploratory Study Using AltspaceVR}}},
  shorttitle = {Investigating the {{Influence}} of {{Intergroup Contact}} in {{Virtual Reality}} on {{Empathy}}},
