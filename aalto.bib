@article{fixDevelopmentApplicationAirborne2019,
  ids = {fixDevelopmentApplicationAirborne2019a},
  title = {Development and Application of an Airborne Differential Absorption Lidar for the Simultaneous Measurement of Ozone and Water Vapor Profiles in the Tropopause Region},
  author = {Fix, Andreas and Steinebach, Felix and Steinebach, Felix and Wirth, Martin and Sch{\"a}fler, Andreas and Ehret, Gerhard},
  year = {2019},
  month = aug,
  journal = {Applied Optics},
  volume = {58},
  number = {22},
